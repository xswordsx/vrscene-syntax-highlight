{
    "name": "vrscene-syntax-highlight",
    "displayName": "VRScene Syntax Highlight",
    "description": "Language support for Chaos' .vrscene files.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.0.0"
    },
    "publisher": "xswordsx",
    "license": "SEE LICENSE IN LICENSE",
    "categories": ["Programming Languages"],
    "contributes": {
        "languages": [{
            "id": "vrscene",
            "aliases": ["VRScene", "vrscene"],
            "extensions": [".vrscene"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "vrscene",
            "scopeName": "source.vrscene",
            "path": "./syntaxes/vrscene.tmLanguage"
        }]
    }
}